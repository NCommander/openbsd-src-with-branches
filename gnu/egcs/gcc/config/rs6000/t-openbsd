# include t-rs6000 too
# this is taken from t-ppcos

MULTILIB_MATCHES_FLOAT	= msoft-float=mcpu?403 \
			  msoft-float=mcpu?mpc403 \
			  msoft-float=mcpu?ppc403
MULTILIB_MATCHES        = ${MULTILIB_MATCHES_FLOAT} \
                          fPIC=mrelocatable-lib \
                          fPIC=mrelocatable \
                          fPIC=fpic           

