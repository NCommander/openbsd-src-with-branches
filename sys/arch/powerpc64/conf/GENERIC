machine		powerpc64
include		"../../../conf/GENERIC"
maxusers	80

option		PCIVERBOSE

config		bsd	swap generic

mainbus0	at root
cpu0		at mainbus?
phb*		at fdt?
pci*		at phb?
ppb*		at pci?
pci*		at ppb?
