#!/bin/sh -
#
#	@(#)monthly	5.5 (Berkeley) 6/17/91
#

host=`hostname -s`
echo "Subject: $host monthly run output"

if [ -f /etc/monthly.local ];then
	echo ""
	echo "Running monthly.local:"
	. /etc/monthly.local
else
	echo "Nothing to do!"
fi

